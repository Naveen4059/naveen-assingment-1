   "source": [
    "list1 = [20,10,12,15,16,14,18]\n",
    "list2 = [5,2,4,3,8,7,9]\n",
    "total_diff = 0\n",
    "for i in range(len(list1)):\n",
    "    if list1[i] >= list2[i]:\n",
    "        diff = list1[i] - list2[i]\n",
    "    else:\n",
    "        diff = list2[i] - list1[i]\n",
    "    total_diff += diff\n",
    "print(\"Sum of element-wise differences\",total_diff)\n"
   ]
